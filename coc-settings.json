{
  "suggest.noselect": true,
  "suggest.removeDuplicateItems": true,
  "coc.preferences.hoverTarget": "echo",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "javascript",
    "javascriptreact",
    "json",
    "Markdown",
    "markdown",
    "markdownlint",
    "go",
    "gomod",
    "python",
    "ruby",
    "typescript",
    "terraform",
    "typescript.tsx",
    "typescriptreact",
    "yaml"
  ],
  "python.linting.enabled": false,
  "python.jediEnabled": false,
  "python.linting.pylintPath": "/Users/william/.asdf/shims/pylint",
  "python.formatting.provider": "yapf",
  "yaml.format.enable": true,
  "yaml.schemas": {
    "http://json.schemastore.org/swagger-2.0": "swagger.yaml",
    "https://jsonapi.org/schema": "swagger/**/*",
    "https://json.schemastore.org/circleciconfig": ".circleci/config.yml"
  },
  "solargraph.autoformat": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,
  "markdownlint.onOpen": false,
  "markdownlint.onChange": false,
  "markdownlint.onSave": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve",
        "-tf-exec=/Users/william/.asdf/shims/terraform"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
